# Prime-Sparse SaaS Configuration
# Copy this to .env and fill in values

# Application
APP_ENV=development
DEBUG=true
SECRET_KEY=your-super-secret-key-change-in-production
API_V1_PREFIX=/api/v1

# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/primesparse
DATABASE_POOL_SIZE=20
DATABASE_MAX_OVERFLOW=10

# Redis
REDIS_URL=redis://localhost:6379/0
REDIS_PASSWORD=

# Celery
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# S3/MinIO Storage
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=prime-sparse-models
S3_REGION=us-east-1

# JWT Authentication
JWT_SECRET_KEY=your-jwt-secret-key-change-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Stripe (Payments)
STRIPE_SECRET_KEY=sk_test_xxxxx
STRIPE_PUBLISHABLE_KEY=pk_test_xxxxx
STRIPE_WEBHOOK_SECRET=whsec_xxxxx
STRIPE_PRICE_PRO=price_xxxxx
STRIPE_PRICE_ENTERPRISE=price_xxxxx

# Sentry (Error Tracking)
SENTRY_DSN=

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Rate Limiting
RATE_LIMIT_FREE=10/minute
RATE_LIMIT_PRO=60/minute
RATE_LIMIT_ENTERPRISE=300/minute

# Model Optimization
MAX_MODEL_SIZE_FREE_MB=500
MAX_MODEL_SIZE_PRO_MB=5000
MAX_MODEL_SIZE_ENTERPRISE_MB=50000
DEFAULT_SPARSITY=0.96
